{
  "name": "jyson",
  "version": "3.1.2",
  "description": "A template engine for json.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha -R spec --recursive \"./spec/**/*.spec.js\"",
    "test:watch": "npm test -- --watch",
    "test:lint": "./node_modules/.bin/eslint .",
    "test:coverage": "./node_modules/.bin/nyc --reporter=html --reporter=text npm test",
    "report:coverage": "./node_modules/.bin/nyc report --reporter=text-lcov | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubba/jyson.git"
  },
  "keywords": [
    "json",
    "template"
  ],
  "author": "Edward Andrew Robinson <earobinson@hubba.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hubba/jyson/issues"
  },
  "homepage": "https://github.com/hubba/jyson#readme",
  "devDependencies": {
    "@ear/eslint-config": "^1.2.0",
    "chai": "^4.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.19.1",
    "lodash.set": "^4.3.2",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sinon": "^4.5.0",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isfunction": "^3.0.9"
  },
  "nyc": {
    "exclude": "**/*.spec.js"
  }
}
